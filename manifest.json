{

    "manifest_version": 2,
    "name": "uchadify",
    "version": "1.0",
  
    "description": "Chadify the U-Cursos website.",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.u-cursos.cl/*"],
        "js": ["scripts/chadify.js"]
      },
      {
        "matches": ["*://*.u-cursos.cl/ingenieria/*/*/*/*/tareas/*"],
        "js": ["scripts/hours.js"]
      }
    ]
  
  }
  